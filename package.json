{
  "name": "@creditiq/web-base",
  "version": "1.0.1",
  "main": "dist/main.js",
  "typings": "dist/lib/index.d.ts",
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "preversion": "npm run build:lib",
    "clean": "rm -rf dist",
    "precompile": "npm run clean",
    "compile": "node_modules/.bin/tsc -p . && npm run compile:babel",
    "compile:babel": "node_modules/.bin/babel dist -d dist --source-maps inline",
    "compile:watch": "node_modules/.bin/watch --filter=watch-filter.js 'npm run compile' src",
    "watch": "npm-run-all --parallel test:watch:ci compile:watch",
    "start": "npm run dev",
    "dev": "npm-run-all --parallel open:src test:watch",
    "dev:offline": "npm-run-all --parallel 'open:src -- --offline' test:watch",
    "dev:demo": "npm-run-all --parallel 'open:src -- --demo' test:watch",
    "dev:local": "npm-run-all --parallel 'open:src -- --apiStage local' test:watch",
    "dev:dev": "npm-run-all --parallel 'open:src -- --apiStage dev' test:watch",
    "dev:staging": "npm-run-all --parallel 'open:src -- --apiStage staging' test:watch",
    "open:src": "babel-node --presets env tools/srcServer.js",
    "open:dist": "babel-node --presets env tools/distServer.js",
    "pretest": "npm run compile",
    "prebuild": "npm run clean && npm run test",
    "build": "babel-node --presets env tools/build.js",
    "prebuild:lib": "npm run prebuild",
    "build:lib": "babel-node --presets env tools/buildLib.js",
    "build:lib:watch": "babel-node --presets env tools/buildLib.js --watch",
    "build:server": "npm run build && npm run open:dist",
    "test": "jest --config jest.single.config.js",
    "test:watch": "jest --watch",
    "test:watch:ci": "CI=true npm run test:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creditiq/web-base.git"
  },
  "dependencies": {
    "@creditiq/style": "^5.5.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@creditiq/console-state": "^10.4.0",
    "@creditiq/item": "^18.3.0",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.5",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^20.0.8",
    "@types/lodash": "^4.14.76",
    "@types/node": "^7.0.12",
    "@types/react": "^16.0.20",
    "@types/react-dom": "^16.0.2",
    "@types/react-redux": "^5.0.12",
    "auth0-lock": "^11.0.0-beta.9",
    "autoprefixer": "^8.6.3",
    "babel-cli": "^6.18.0",
    "babel-core": "~6.24.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-display-name": "~2.0.0",
    "babel-plugin-transform-react-constant-elements": "~6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "~0.2.9",
    "babel-polyfill": "~6.16.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-1": "~6.13.0",
    "browser-sync": "~2.14.0",
    "chalk": "~1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "convert-source-map": "1.3.0",
    "copy-webpack-plugin": "~3.0.1",
    "css-loader": "^0.28.7",
    "ejs-compiled-loader": "~1.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.1",
    "jest-cli": "^21.1.0",
    "jest-enzyme": "^4.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.18.2",
    "transform-ts-paths": "^1.0.1",
    "ts-jest": "^20.0.11",
    "ts-loader": "^4.2.0",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-language-service": "^0.9.6",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.4",
    "typescript-fsa": "^2.5.0",
    "typings-for-css-modules-loader": "^1.6.1",
    "url-loader": "^1.0.1",
    "validate-commit-msg": "^2.14.0",
    "watch": "^1.0.2",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-fail-plugin": "^2.0.0",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-md5-hash": "~0.0.5"
  },
  "engines": {
    "node": ">=6.10.x"
  },
  "description": "placeholder",
  "license": "UNLICENSED",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "ciq": {
    "upstream": "git@github.com:creditiq/base.git"
  }
}
