{
  "name": "@creditiq/web-base",
  "version": "1.0.1",
  "main": "dist/main.js",
  "typings": "dist/lib/index.d.ts",
  "scripts": {
    "postinstall": "! pwd | grep -q node_modules && patch-package",
    "precommit": "ciq package check-file-links",
    "commitmsg": "validate-commit-msg",
    "preversion": "npm run build:lib",
    "clean": "rm -rf dist",
    "precompile": "npm run clean",
    "compile": "node_modules/.bin/tsc -p .",
    "compile:watch": "node_modules/.bin/watch --filter=watch-filter.js 'npm run compile' src",
    "watch": "npm-run-all --parallel test:watch compile:watch",
    "start": "npm run dev",
    "dev": "npm-run-all --parallel open:src test:watch",
    "dev:offline": "npm-run-all --parallel 'open:src -- --offline' test:watch",
    "dev:demo": "npm-run-all --parallel 'open:src -- --demo' test:watch",
    "dev:local": "npm-run-all --parallel 'open:src -- --apiStage local' test:watch",
    "dev:dev": "npm-run-all --parallel 'open:src -- --apiStage dev' test:watch",
    "dev:staging": "npm-run-all --parallel 'open:src -- --apiStage staging' test:watch",
    "open:src": "node tools/srcServer.js",
    "open:dist": "node tools/distServer.js",
    "pretest": "npm run compile",
    "prebuild": "npm run clean && npm run test",
    "build": "node tools/build.js",
    "prebuild:lib": "npm run prebuild",
    "build:lib": "node tools/buildLib.js",
    "build:lib:watch": "node tools/buildLib.js --watch",
    "build:server": "npm run build && npm run open:dist",
    "test": "jest --config jest/jest.single.config.js",
    "test:watch": "jest --watch --config jest/jest.config.js",
    "test:watch:ci": "CI=true npm run test:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creditiq/web-base.git"
  },
  "dependencies": {
    "@creditiq/style": "^7.9.0",
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.5",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.14.7",
    "@types/react": "^16.0.20",
    "@types/react-dom": "^16.0.2",
    "ajv": "^6.5.5",
    "autoprefixer": "^8.6.3",
    "browser-sync": "~2.14.0",
    "chalk": "~1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "convert-source-map": "1.3.0",
    "copy-webpack-plugin": "~3.0.1",
    "css-loader": "^0.28.7",
    "ejs-compiled-loader": "~1.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-enzyme": "^4.0.1",
    "lodash": "^4.17.4",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.1",
    "patch-package": "^6.2.0",
    "postcss-loader": "^2.0.6",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.18.2",
    "transform-ts-paths": "^1.0.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.2.0",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.0.0",
    "typescript": "^3.7.2",
    "url-loader": "^1.0.1",
    "validate-commit-msg": "^2.14.0",
    "watch": "^1.0.2",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-md5-hash": "~0.0.5"
  },
  "engines": {
    "node": ">=6.10.x"
  },
  "description": "placeholder",
  "license": "UNLICENSED",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "ciq": {
    "upstream": "git@github.com:creditiq/base.git"
  }
}
